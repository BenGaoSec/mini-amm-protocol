{
  "printWidth": 100,
  "tabWidth": 4,
  "singleQuote": false,
  "semi": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "plugins": ["prettier-plugin-solidity"],
  "overrides": [
    {
       "files": [
        "src/**/*.sol",
        "script/**/*.sol",
        "test/**/*.sol"
      ],
      "options": {
        "printWidth": 100,
        "tabWidth": 4
      }
    }
  ]
}
